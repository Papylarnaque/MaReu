<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mareu">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ma_reu"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ma_reu"
        android:supportsRtl="true"
        android:theme="@style/MaRéu">
        <activity
            android:name=".view.SplashScreen"
            android:noHistory="true">
            <intent-filter>

                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.AddReunionActivity"
            android:parentActivityName=".view.ListActivity"
            android:theme="@style/MaRéu.NoActionBar" />


        <activity
            android:name=".view.ListActivity"
            android:label="@string/app_name"
            android:theme="@style/MaRéu.NoActionBar"
            />
    </application>

</manifest>
